{"name":"#[CF_tempEntity]","type":"character","img":"icons/svg/mystery-man.svg","data":{"attributes":{"agility":{"die":{"sides":4,"modifier":0},"wild-die":{"sides":6}},"smarts":{"die":{"sides":4,"modifier":0},"wild-die":{"sides":6},"animal":false},"spirit":{"die":{"sides":4,"modifier":0},"wild-die":{"sides":6},"unShakeBonus":0},"strength":{"die":{"sides":4,"modifier":0},"wild-die":{"sides":6},"encumbranceSteps":0},"vigor":{"die":{"sides":4,"modifier":0},"wild-die":{"sides":6}}},"stats":{"speed":{"runningDie":6,"runningMod":0,"value":6},"toughness":{"value":0,"armor":0,"modifier":0},"parry":{"value":0,"modifier":0},"size":0},"details":{"biography":{"value":""},"species":{"name":""},"archetype":"","currency":500,"wealth":{"die":6,"modifier":0,"wild-die":6},"conviction":{"value":0,"active":false},"autoCalcToughness":true,"autoCalcParry":true},"powerPoints":{},"fatigue":{"value":0,"min":0,"max":2},"wounds":{"value":0,"min":0,"max":3,"ignored":0},"advances":{"value":0,"rank":"Novice","details":""},"bennies":{"value":3,"max":3},"status":{"isShaken":false,"isDistracted":false,"isVulnerable":false,"isStunned":false,"isEntangled":false,"isBound":false},"initiative":{"hasHesitant":false,"hasLevelHeaded":false,"hasImpLevelHeaded":false,"hasQuick":false},"additionalStats":{},"wildcard":true},"token":{"name":"#[CF_tempEntity]","img":"icons/svg/mystery-man.svg","displayName":0,"actorLink":false,"width":1,"height":1,"scale":1,"mirrorX":false,"mirrorY":false,"lockRotation":false,"rotation":0,"alpha":1,"vision":false,"dimSight":0,"brightSight":0,"sightAngle":0,"light":{"alpha":0.5,"angle":0,"bright":0,"coloration":1,"dim":0,"gradual":true,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"speed":5,"intensity":5,"reverse":false},"darkness":{"min":0,"max":1}},"disposition":-1,"displayBars":0,"bar1":{"attribute":null},"bar2":{"attribute":null},"flags":{},"randomImg":false,"actorId":null},"items":[{"_id":"6nZk5RtWVTtyotyQ","name":"Persuasion","type":"skill","img":"sy